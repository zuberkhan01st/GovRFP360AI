# GovRFP360AI Backend Environment Variables

# Server Configuration
PORT=5000
NODE_ENV=development

# Vector Database Configuration (Weaviate)
WEAVIATE_URL=https://your-cluster.weaviate.network
WEAVIATE_API_KEY=your_weaviate_api_key_here
# Legacy environment variables for backward compatibility  
VECTOR_DB_API_KEY=your_weaviate_api_key_here
VECTOR_DB_ENVIRONMENT=your_weaviate_cluster_url_here
# Example: VECTOR_DB_ENVIRONMENT=cluster-123.weaviate.network

# AI Service API Keys
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
COHERE_API_KEY=your_cohere_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# LLM Provider Selection (gemini, openai, groq)
LLM_PROVIDER=gemini

# Optional: Additional AI Service Configurations
ANTHROPIC_API_KEY=your_anthropic_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Database Configuration (if using additional databases)
# DATABASE_URL=your_database_url_here

# Security
JWT_SECRET=your_jwt_secret_here

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# Logging
LOG_LEVEL=info

# File Upload Settings
MAX_FILE_SIZE=10mb

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100

# Cache Settings
CACHE_TTL=3600mple .env file for GovRFP360AI backend
